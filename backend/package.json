{
  "name": "quick-finder",
  "productName": "QuickFinder",
  "version": "0.1.0",
  "description": "Quickly search files, applications, and run commands!",
  "main": ".webpack/main",
  "scripts": {
    "start": "cross-env BUILD_ENV=development electron-forge start",
    "package": "cross-env BUILD_ENV=production electron-forge package",
    "make": "cross-env BUILD_ENV=production electron-forge make",
    "publish": "electron-forge publish",
    "format": "bun prettier . --write",
    "lint": "bunx eslint",
    "fix-lint": "bunx eslint --fix"
  },
  "devDependencies": {
